{
    "name": "lekoala/silverstripe-geotools",
    "description": "Geo tools for SilverStripe",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "geo",
        "module",
        "cms"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "LeKoala",
            "email": "thomas@lekoala.be"
        }
    ],
    "require": {
        "php": "^8",
        "silverstripe/recipe-plugin": "^2",
        "silverstripe/vendor-plugin": "^2",
        "silverstripe/framework": "^5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "extra": {
        "installer-name": "geotools"
    },
    "autoload": {
        "psr-4": {
            "LeKoala\\GeoTools\\": "src/",
            "LeKoala\\GeoTools\\Test\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/lekoala/silverstripe-geotools/issues"
    },
    "scripts": {
        "lint": "phpcs src/ tests/",
        "lint-clean": "phpcbf src/ tests/",
        "test": "phpunit -v"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
